        <servlet-name>resorts</servlet-name>
        <url-pattern>/resorts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testservlet</servlet-name>
        <url-pattern>/testservlet</url-pattern>
