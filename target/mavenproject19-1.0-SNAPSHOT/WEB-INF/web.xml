        <servlet-class>mybm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>places</servlet-name>
        <servlet-class>places</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>resorts</servlet-name>
        <servlet-class>resorts</servlet-class>
